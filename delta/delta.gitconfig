[include]
        path = themes.gitconfig

[core]
        pager = delta

[interactive]
        diffFilter = delta --color-only

[delta]
        navigate = true
        dark = true
        hyperlinks = true
        hyperlinks-file-link-format = "vscode://file/{path}:{line}"

[merge]
        conflictstyle = diff3

[diff]
        colorMoved = default